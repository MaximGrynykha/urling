{
    "name": "ismaxim/urling",
    "description": "URL parser and constructor in PHP",
    "type": "library",
    "keywords": [
        "url", 
        "php",
        "urling", 
        "url parser", 
        "url builder",
        "url constructor" 
    ],
    "homepage": "https://github.com/ismaxim/urling",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Grynykha",
            "email": "gr.gromax@gmail.com",
            "homepage": "https://github.com/ismaxim",
            "role": "lead-developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0",
        "phpstan/phpstan": "^0.12.0",
        "squizlabs/php_codesniffer": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "Urling\\": "src/Urling"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Urling\\": "tests/Urling"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs",
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "config": {
        "process-timeout": 0
    }
}
